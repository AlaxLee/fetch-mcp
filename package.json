{
  "name": "@alaxlee/mcp-rendered-fetch-server",
  "version": "1.2.1-hotfix.1",
  "description": "An MCP server that returns JS-rendered HTML via Playwright (inspired by zcaceres)",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "alax",
  "contributors": [
    "Zach Caceres (zach.dev)",
    "Piotr Wilkin (ilintar@gmail.com)"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "postinstall": "npx playwright install"
  },
  "bin": {
    "mcp-rendered-fetch-server": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "private-ip": "^3.0.2",
    "zod": "^3.24.1",
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "shx": "0.4.0"
  }
}
